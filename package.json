{
  "name": "tsconline-monorepo",
  "packageManager": "yarn@3.5.1",
  "private": true,
  "workspaces": [
    "app",
    "shared",
    "server"
  ],
  "scripts": {
    "build": "echo \"Building all workspaces...\" && yarn workspace @tsconline/shared run build && yarn workspaces foreach -p -v run build",
    "start": "yarn workspace @tsconline/server start"
  },
  "dependencies": {
    "fastify": "^4.13.0",
    "fs": "^0.0.1-security",
    "nvm": "^0.0.4",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@yarnpkg/sdks": "^3.0.0-rc.43"
  }
}
